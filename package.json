{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  "name": "sellermockups-ai-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js"
  },
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ]
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "@google/generative-ai": "^0.17.0"
  },
  "engines": {
    "node": "20.x"
  }
}
